<h1>Edit a New Landmark</h1>

<form action="/landmarks/<%= @landmark.id %>" method="post">
  <input type="hidden" id="hidden" name="_method" value="patch">
  <label for="landmark_name">Landmark Name: </label>
  <input type="text" name="landmark[name]" id="name" value="<%= @landmark.name %>"><br><br>

  <label for="year_completed">Year Completed: </label>
  <input type="text" name="landmark[year_completed]" id="year_completed" value="<%= @landmark.year_completed %>"><br><br>

  <input type="submit" name="" value="Edit Landmark">
</form>
